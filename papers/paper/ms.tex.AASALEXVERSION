\title{Ages of Stars and Planets in the Kepler Field Over the First Four Billion Years}

\correspondingauthor{Luke G. Bouma}
\email{bouma.luke@gmail.com}

\received{2024 June 21}
\revised{2024 September 4}
\accepted{---}
\shorttitle{Kepler's Demographic Cliff} 

\shortauthors{Bouma et al.}

\author[0000-0002-0514-5538]{Luke~G.~Bouma}
\altaffiliation{51 Pegasi b Fellow;  Carnegie Fellow}
\affiliation{\caltech}
\affiliation{\carnegie}

\author{Lynne~A.~Hillenbrand}
\affiliation{\caltech}

\author[0000-0001-8638-0320]{Andrew W. Howard}
\affiliation{\caltech}

